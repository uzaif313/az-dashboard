{
  "name": "az-dash",
  "version": "1.0.0",
  "publisher": "uzaif",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/uzaif313/az-dash"
  },
  "engines": {
    "vscode": "^1.25.0"
  },
  "main": "./out/extension",
  "activationEvents": [
    "onCommand:az.dash"
  ],
  "contributes": {
    "commands": [
      {
        "command": "az.dash",
        "title": "Az-Dashboard!!"
      }
    ]
  },
  "scripts": {
    "compile": "npm-run-all compile:*",
    "compile:views": "webpack --mode development",
    "watch": "npm-run-all -p watch:*",
    "watch:views": "webpack --watch --mode development",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "vscode:prepublish": "yarn compile",
    "package": "vsce package"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^5.1.2",
    "reactstrap": "^8.2.0",
    "recharts": "^2.0.0-beta.1",
    "vscode": "^1.1.18"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@types/node": "^10.5.2",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "babel-loader": "8.3.0",
    "css-loader": "^6.7.3",
    "less": "^3.7.0",
    "less-loader": "^11.1.0",
    "npm-run-all": "^4.1.3",
    "style-loader": "^3.3.1",
    "styled-components": "^3.3.3",
    "ts-import-plugin": "^1.5.4",
    "ts-loader": "^9.4.2",
    "typescript": "^2.9.2",
    "vsce": "^1.71.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
